<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OrderManagement">

    <RelativeLayout
        android:layout_width="wrap_content"
        android:id="@+id/re"
        android:layout_height="wrap_content"
        tools:ignore="MissingConstraints">



        <TextView
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:text="Order Management"
            android:textAlignment="center"
            android:gravity="center_vertical"
            android:textColor="#fff"
            android:textStyle="bold"
            android:textSize="20sp"
            android:background="#5C25B7"
            android:id="@+id/tex"
            />
        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="28dp"
            android:layout_height="60dp"
            android:src="@drawable/ic_arrow_back_black_24dp"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp" />



    </RelativeLayout>

    <TextView

        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="Choose your fundtion by clicking the button below"
        android:textAlignment="center"
        android:textColor="#000"
        android:textStyle="bold"
        android:id="@+id/tr"
        android:layout_below="@+id/re"
        android:layout_marginTop="20dp"
        tools:ignore="MissingConstraints" />




    <!--  <TextView
          android:layout_width="50dp"
          android:layout_height="wrap_content"
          android:text="."
          android:textColor="#000"
          android:textSize="100dp"
          android:layout_below="@+id/one"/>-->

    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/dot"
        android:id="@+id/dot1"
        android:layout_below="@+id/tr"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/dot1"
        android:text="View Upcoming Orders"
        android:layout_below="@+id/tr"
        android:layout_marginTop="18dp"
        android:layout_marginStart="10dp"
        android:textColor="#000"/>
    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/dot"
        android:id="@+id/dot2"
        android:layout_below="@+id/dot1"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/dot2"
        android:text="View Order History"
        android:layout_below="@+id/dot1"
        android:layout_marginTop="18dp"
        android:layout_marginStart="10dp"
        android:textColor="#000"/>

    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/dot"
        android:id="@+id/dot3"
        android:layout_below="@+id/dot2"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/dot3"
        android:text="View Subscription list"
        android:layout_below="@+id/dot2"
        android:layout_marginTop="18dp"
        android:layout_marginStart="10dp"
        android:textColor="#000"/>

    <ImageView
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:src="@drawable/dot"
        android:id="@+id/dot4"
        android:layout_below="@+id/dot3"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/dot4"
        android:text="View Cancelled Subscription"
        android:layout_below="@+id/dot3"
        android:layout_marginTop="18dp"
        android:layout_marginStart="10dp"
        android:textColor="#000"/>


    <com.sa90.materialarcmenu.ArcMenu
        android:id="@+id/arcMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:menu_color="@color/blu"
        android:layout_centerInParent="true"
        android:layout_marginTop="30dp"
        app:menu_open="arc_left"
        app:menu_scr="@drawable/organization">

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:id="@+id/fab1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:menu_color="@color/blu"
            android:src="@drawable/ordersssss" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            app:menu_color="@color/blu"
            android:id="@+id/tvNext"
            android:layout_height="wrap_content"
            android:src="@drawable/orderhis" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            app:menu_color="@color/blu"
            android:id="@+id/tvAnimationDemo"
            android:src="@drawable/contract"
            android:layout_height="wrap_content" />

        <com.google.android.material.floatingactionbutton.FloatingActionButton
            android:layout_width="wrap_content"
            app:menu_color="@color/blu"
            android:id="@+id/tvTopPlacement"
            android:src="@drawable/cancelfgh"
            android:layout_height="wrap_content" />




    </com.sa90.materialarcmenu.ArcMenu>
</RelativeLayout>